{
  "manifest_version": 3,
  "name": "Ultra Reminder",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],  
      "all_frames": true,
      "js": [
        "src/script/script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ]
}